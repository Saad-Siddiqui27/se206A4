<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.media.MediaView?>

<AnchorPane fx:id="revise" prefHeight="600.0" prefWidth="600.0" stylesheets="@../CSS/Revise.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Revise">
   <children>
      <MediaView fx:id="media" fitHeight="400.0" fitWidth="500.0" layoutX="9.0" layoutY="14.0" />
      <Button fx:id="next" layoutX="558.0" layoutY="548.0" mnemonicParsing="false" onAction="#go" text="Take Quiz" />
      <Button layoutX="40.0" layoutY="548.0" mnemonicParsing="false" onAction="#switchToMain" text="MainMenu" />
      <Button fx:id="confirmButton" layoutX="288.0" layoutY="503.0" mnemonicParsing="false" onAction="#confirm" text="Confirm" />
      <TextField fx:id="ans" alignment="CENTER" layoutX="239.0" layoutY="461.0" />
      <Label fx:id="points" layoutX="511.0" layoutY="22.0" prefHeight="16.0" prefWidth="95.0" />
      <Label fx:id="wrong" layoutX="249.0" layoutY="553.0" prefHeight="30.0" prefWidth="200.0" />
   </children>
</AnchorPane>
