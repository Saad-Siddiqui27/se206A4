<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.media.MediaView?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="_learning" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ActivelearningController">
   <children>
      <Button layoutX="634.0" layoutY="439.0" mnemonicParsing="false" onAction="#check" text="Answer" />
      <TextField fx:id="_answer" layoutX="446.0" layoutY="439.0" promptText="Enter the answer:" />
      <ListView fx:id="_videos" layoutX="53.0" layoutY="74.0" prefHeight="378.0" prefWidth="200.0" />
      <Text layoutX="53.0" layoutY="53.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Please choose a video:" wrappingWidth="310.0">
         <font>
            <Font size="19.0" />
         </font>
      </Text>
      <Button layoutX="53.0" layoutY="533.0" mnemonicParsing="false" onAction="#switchToMain" prefHeight="33.0" prefWidth="128.0" text="Main menu" />
      <Button layoutX="332.0" layoutY="93.0" mnemonicParsing="false" onAction="#play" prefHeight="33.0" prefWidth="66.0" text="Play" />
      <MediaView fx:id="_view" fitHeight="300.0" fitWidth="300.0" layoutX="467.0" layoutY="46.0" />
      <Text layoutX="446.0" layoutY="405.0" strokeType="OUTSIDE" strokeWidth="0.0" text="What is the video talking about?">
         <font>
            <Font size="18.0" />
